def filaA(a,b,c):
    return (a*b+c)
def filaB(a,b,c):
    return (a-b+c)
def filaC(a,b,c):
    return (a+b*c)


def columA (a,b,c):
    return(a*b-c)
def columnB (a,b,c):
    return(a+b-c)
def columC(a,b,c):
    return(a+b/c)
